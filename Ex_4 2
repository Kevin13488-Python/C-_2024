#include <iostream>

int main()
{
    int x,y;
    printf("pls input int x=");
    scanf("%d",&x);
    switch(x){
        case 0:
        printf("Sunday \n");
        break;
        case 1:
        printf("Saturday \n");
        break;
        case 2:
        printf("Monday \n");
        break;
        case 3:
        printf("Tuesday \n");
        break;
        case 4:
        printf("Wednesday \n");
        break;
        case 5:
        printf("Thursday \n");
        break;
        case 6:
        printf("Friday \n");
        break;
    }
}
